{
    "name": "Scantor QR Code Extension",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "128": "scantor.jpg"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
      }
    ]
  }